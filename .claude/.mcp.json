{
    "mcpServers": {
        "moon": {
            "command": "moon",
            "args": ["mcp"],
            "env": {
                "MOON_WORKSPACE_ROOT": "/absolute/path/to/your/moon/workspace"
            }
        },
        "shadcn": {
            "command": "npx",
            "args": ["shadcn@latest", "mcp"]
        }
    }
}
